{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gb-up/card",
  "title": "GB up card",
  "category": "text",
  "icon": "email",
  "description": "A customizable card block.",
  "keywords": ["card", "title", "text", "gb"],
  "textdomain": "gb-up",
  "attributes": {
    "content": {
      "type": "string",
      "default": "Gb up card"
    },
    "level": {
      "type": "number",
      "default": 2
    },
    "align": {
      "type": "string",
      "default": "left"
    },
    "color": {
      "type": "string",
      "default": "#444"
    }
  },
  "supports": {
    "align": ["left", "center", "right"]
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
