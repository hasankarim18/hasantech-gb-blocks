{
  "name": "hasan/hasantech-gb-blocks",
  "description": "Collection of gutenberg blocks by hasantech.xyz",
  "autoload": {
    "psr-4": {
      "Hasan\\HasantechGbBlocks\\": "src/"
    }
  },
  "scripts": {
    "dev": [
      "composer install"
    ],
    "build": [
      "@composer --no-plugins --no-interaction install --no-scripts --no-dev"
    ]
  },
  "authors": [
    {
      "name": "hasankarim18",
      "email": "hallysust@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">= 8.0"
  },
  "require-dev": {
    "php-stubs/wordpress-stubs": "^6.8",
    "arifpavel/elementor-stubs": "3.1.4.1"
  }
}
